    <h2 id="toolbar">Toolbar</h2>
    <p>Icon toolbar for application actions.</p>

    <%- example(`
      <div class="toolbar">
        <button class="toolbar-button">
          <span>Back</span>
        </button>
        <button class="toolbar-button">
          <span>Forward</span>
        </button>
        <div class="toolbar-separator"></div>
        <button class="toolbar-button">
          <span>Refresh</span>
        </button>
        <button class="toolbar-button" disabled>
          <span>Stop</span>
        </button>
      </div>
    `) %>

