    <!-- Scrollbar Demo in Sidebar -->
    <div style="padding: 8px; background: var(--surface-light); border: 1px solid #888; border-radius: 5px;">
      <div style="font-size: 11px; font-weight: bold; margin-bottom: 6px;">Scrollbar Preview</div>
      <div class="scrollbar-demo">
        <p style="margin: 0 0 8px 0;">Scroll me!</p>
        <p style="margin: 0 0 8px 0;">Item 1</p>
        <p style="margin: 0 0 8px 0;">Item 2</p>
        <p style="margin: 0 0 8px 0;">Item 3</p>
        <p style="margin: 0 0 8px 0;">Item 4</p>
        <p style="margin: 0 0 8px 0;">Item 5</p>
        <p style="margin: 0 0 8px 0;">Item 6</p>
